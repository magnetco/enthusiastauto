# Sanity CMS Configuration
# Copy this file to .env and fill in your actual values

# Your Sanity project ID (found in sanity.json or Sanity dashboard)
SANITY_PROJECT_ID=your_project_id_here

# Dataset to sync to (usually 'production' or 'development')
SANITY_DATASET=production

# Sanity API token with write permissions
# Generate at: https://www.sanity.io/manage/personal/tokens
SANITY_API_TOKEN=your_write_token_here

# Sanity API version (leave as default unless you have a specific reason to change)
SANITY_API_VERSION=2021-06-07
